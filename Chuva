#include <stdio.h>

int main() {
     
     int i = 0;
     int j = 0;
     int m = 0;
     int n = 0;
     int k = 0;
     int h = 0;
     int qtd = 0;
     int rep = 0;   
    
    scanf("%d", &qtd);
  
    m = qtd;
    n = qtd;
    
    int matriz[m][n];
    int matriz2[m][n];
    int matriz3[m][n];
  
    for(i = 0; i < m; i++){
      for(j = 0; j < n; j++){
        scanf("%d", &matriz[i][j]);
      }
    }

    for(i = 0; i < m; i++){
      for(j = 0; j < n; j++){
        scanf("%d", &matriz2[i][j]);
      }
    }

    for(i = 0; i < m; i++){
      for(j = 0; j < n; j++){
        matriz3[i][j] = matriz[i][j] + matriz2[i][j];
          printf("%d ", matriz3[i][j]);
      }
      printf("\n");
    }

     //  for(i = 0; i < m; i++){
     //    printf("\n");
     //   for(j = 0; j < n-1; j++){ 
     //     printf("%d ", matriz3[i][j]);
     //   }printf("%d", matriz3[i][j]);
     // }   
    
  return 0;
}
