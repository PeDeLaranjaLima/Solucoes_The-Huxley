#include <stdio.h>
#include <math.h>

int main() {

    int n1;
    int n2;
    int cont=0;
    int i;
    
    //Apenas os n?meros positivos menores que 50 devem ser testados
      scanf("%d %d", &n1, &n2);
      
            for(i=1; i<50; i++){
              if(i%n1 == 0 && i%n2 == 0){
                cont++;
              } 
              if(n1 == 0 || n2 == 0){
                cont = 0; 
                printf("%d", cont);
                return 0;
              }
            }
           
        printf("%d", cont);
    
 return 0;
}
