#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  
  int idade, altura;
  int cont = 0;
  
  scanf("%d %d", &altura, &idade);


  //Barca Viking
  if(altura >=  150 && idade >= 12){
    cont++; 
  }
  //Elevator of Death
  if(altura >=  140 && idade >= 14){
    cont++; 
  }
  //Final Killer
  if(altura >=  170 || idade >= 16){
    cont++; 
  }

  printf("%d", cont);
  
return 0;
}
