#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    float n1;
    float n2;
    float n3;
    int cont = 0; 

    scanf("%f%f%f", &n1, &n2, &n3);

    int media = (n1+n2+n3)/3; 
    

    if (n1 > media){

    cont = cont+1; 
    
    } 
    if (n2 > media){

    cont = cont+1; 
    
    }  

    if (n3 > media){

    cont = cont+1; 
    
    } 

    printf("%d", cont); 

	return 0;
}
