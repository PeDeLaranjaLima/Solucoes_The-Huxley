#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){

    int num=0;
    int cont=0;
    
    scanf("%d", &num);
    
    while (cont<num){
        cont++;
         if(!(cont%2==0 && cont%7==0)){
            printf("%d\n", cont);
        }
    }
    return 0;

}
